{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "create-project",
    "*"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:allow-open",
    "fs:default",
    "dialog:allow-confirm",
    "dialog:default",
    "fs:allow-create",
    "fs:allow-write-text-file",
    "fs:allow-mkdir",
    "fs:allow-exists",
    "fs:allow-write-text-file",
    "fs:create-app-specific-dirs",
    "fs:allow-write-text-file",
    "fs:read-files",
    "fs:write-files",
    "fs:allow-write-text-file",
    "fs:allow-read-file",
    "fs:allow-write",
    "fs:allow-write-file",
    "fs:write-all",
    "fs:read-all",
    "store:default",
    "fs:read-dirs",
    "fs:allow-read-dir",
    "fs:read-meta",
    "core:window:allow-start-dragging",
    "fs:allow-write-text-file",
    "opener:allow-open-path",
    "opener:allow-open-url",
    "opener:allow-reveal-item-in-dir",
    "opener:allow-default-urls",
    "core:path:allow-basename",
    "fs:allow-appdata-read",
    "fs:allow-appdata-write",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$APPDATA"
        },
        {
          "path": "$APPDATA/**"
        },
        {
          "path": "$APPDATA/*"
        },
        {
          "path": "$APPDATA/"
        },
        {
          "path": "$DOCUMENT/**"
        },
        {
          "path": "$DOCUMENT/*"
        },
        {
          "path": "$DOCUMENT/"
        },
        {
          "path": "$DOCUMENT"
        },
        {
          "path": "$DOWNLOAD/**"
        },
        {
          "path": "$DOWNLOAD/*"
        },
        {
          "path": "$DOWNLOAD/"
        },
        {
          "path": "$DOWNLOAD"
        },
        {
          "path": "$AUDIO/**"
        },
        {
          "path": "$AUDIO/*"
        },
        {
          "path": "$AUDIO/"
        },
        {
          "path": "$AUDIO"
        },
        {
          "path": "$DESKTOP/**"
        },
        {
          "path": "$DESKTOP/*"
        },
        {
          "path": "$DESKTOP/"
        },
        {
          "path": "$DESKTOP"
        },
        {
          "path": "$HOME/**"
        },
        {
          "path": "$HOME/*"
        },
        {
          "path": "$HOME/"
        },
        {
          "path": "$HOME"
        },
        {
          "path": "$PICTURE/**"
        },
        {
          "path": "$PICTURE/*"
        },
        {
          "path": "$PICTURE/"
        },
        {
          "path": "$PICTURE"
        },
        {
          "path": "$PUBLIC/**"
        },
        {
          "path": "$PUBLIC/*"
        },
        {
          "path": "$PUBLIC/"
        },
        {
          "path": "$PUBLIC"
        },
        {
          "path": "$VIDEO/**"
        },
        {
          "path": "$VIDEO/*"
        },
        {
          "path": "$VIDEO/"
        },
        {
          "path": "$VIDEO"
        }
      ]
    },
    "store:default"
  ]
}